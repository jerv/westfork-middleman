---
title: Projects
---

<div class="window black">
	<div class="container">
		<h1>Projects</h1>
	</div>
</div>

<div class="container row window l-projects">
	<div class="columns twelve">
		<h2 id="new">New</h2>
		<div class="row">
			<% data.projectdata.newest.each do |n| %>
				<div class="columns four">
					<a href="<%= n.slug %>.html">
					  <img src="images/<%= n.images[0] %>">
					  <h5><%= n.title %></h5>
					</a>
				</div>
			<% end %>
		</div>
		<h2 id="apartments">Apartments</h2>
		<div class="row">
			<% data.projectdata.apartments.each do |apart| %>
				<div class="columns four">
					<a href="<%= apart.slug %>.html">
					  <img src="images/<%= apart.images[0] %>">
					  <h5><%= apart.title %></h5>
					</a>
				</div>
			<% end %>
		</div>
		<h2 id="medical">Medical</h2>
		<div class="row">
			<% data.projectdata.medical.each do |med| %>
				<div class="columns four">
					<a href="<%= med.slug %>.html">
					  <img src="images/<%= med.images[0] %>">
					  <h5><%= med.title %></h5>
					</a>
				</div>
			<% end %>
		</div>
		<h2 id="commercial">Commercial</h2>
		<div class="row">
			<% data.projectdata.commercial.each do |com| %>
				<div class="columns four">
					<a href="<%= com.slug %>.html">
					  <img src="images/<%= com.images[0] %>">
					  <h5><%= com.title %></h5>
					</a>
				</div>
			<% end %>
		</div>
		<h2 id="elevators">Man Lift Elevators</h2>
		<div class="row">
			<% data.projectdata.manliftelevators.each do |elev| %>
				<div class="columns four">
					<a href="<%= elev.slug %>.html">
					  <img src="images/<%= elev.images[0] %>">
					  <h5><%= elev.title %></h5>
					</a>
				</div>
			<% end %>
		</div>
	</div>
</div>

<div class="window black row">
  <div class="container max-420 text-center">
    <h5>Lets get in Touch</h5>
    <p>Contact us today!</p>
    <%= link_to 'Contact us', 'contact.html', :class=>'button' %>
  </div>
</div>